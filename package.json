{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "parcel": "^2.15.4",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "start": "parcel public/pages/index.html",
    "build": "rimraf dist && parcel build public/pages/index.html",
    "dev:watch": "onchange 'public/**/*' -- npm run build",
    "dev:serve": "vercel dev",
    "dev": "npm run build && npm-run-all --parallel dev:watch dev:serve"
  
}
}
