  "scripts": {
    "dev": "vite",
    "dependency:install": "npm install && cd api && npm install && cd ../",
    "diagnose:dburl": "dotenv -e .env -- sh -c 'echo $DATABASE_URL'",
    "prisma:migrate": "dotenv -e .env -- node scripts/migrate.js",
